//  Фильтрация таблицы содержимого документа табеля
( function( ) {
  objTimesheetDates.colTdParentTable.empty( );
  <% if @timesheet_dates.present? %>
    objTimesheetDates.colTdParentTable.append( '<%= j render( 'table_timesheet_dates' ) %>' );
    objTimesheetDates.colTbInit( );
  <% end %>
} ).call( this );
