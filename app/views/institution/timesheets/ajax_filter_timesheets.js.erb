( function ajax( ) {
  const parentTable = document.querySelector( '#col_t .parent_table' );

  <% if @timesheets.present? %>
    parentTable.innerHTML = '<%= j render( "table_timesheets" ) %>';
    MyLib.tableSetSession( parentTable );
  <% else %>
    parentTable.innerHTML = '';
  <% end %>

}( ) );
